      },
      "metaDescription": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "description about the company"
        }
      },
      "keywords": {
        "type": "Text",
        "config": {
          "label": "Keywords",
          "placeholder": "keyword1,keyword2,keyword3"

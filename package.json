    "axios": "^0.27.2",
    "gsap": "^3.10.4",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-oembed-container": "^1.0.1"
  },
  "devDependencies": {
